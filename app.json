{
  "expo": {
    "name": "Selah",
    "slug": "selah-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "selah",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "kr.selah.selah",
      "supportsTablet": true,
      "appleTeamId": "5ARDZKSX95",
      "usesAppleSignIn": true,
      "associatedDomains": ["applinks:selah.kr"],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "kr.selah.selah",
      "softwareKeyboardLayoutMode": "pan",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "selah.kr",
              "pathPrefix": "/post"
            },
            {
              "scheme": "https",
              "host": "selah.kr",
              "pathPrefix": "/verse"
            },
            {
              "scheme": "https",
              "host": "selah.kr",
              "pathPrefix": "/user"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 120,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-apple-authentication",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.707707053099-robncnjvk5fidk58dkhsduelgscdemnn"
        }
      ],
      "expo-sqlite",
      "@react-native-community/datetimepicker",
      "expo-notifications",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e1647274-91d5-4c63-812c-8e641bda634e"
      }
    },
    "owner": "jaycho1214"
  }
}
