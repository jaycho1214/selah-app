{
  "cli": {
    "version": ">= 16.32.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_UNSTABLE_METRO_OPTIMIZE_GRAPH": "1",
        "EXPO_UNSTABLE_TREE_SHAKING": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_UNSTABLE_METRO_OPTIMIZE_GRAPH": "1",
        "EXPO_UNSTABLE_TREE_SHAKING": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
